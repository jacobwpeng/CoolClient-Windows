<xlue>
	<control class="Demo.ProgressPage">

		<objtemplate>
			<children>
				<obj class="TextObject">
					<attr>
						<left>10</left>
						<top>10</top>
						<width>48</width>
						<height>14</height>
						<text>滑动条:</text>
					</attr>
					
				</obj>
				<obj id="slider" class="Thunder.SliderBar">
					<attr>
						<left>10+50</left>
						<top>10</top>
						
						<height>6</height>
						<width>300</width>
						<Type>0</Type>
					</attr>
					<eventlist>
						<event name="OnSliderChange" file="ProgressPage.xml.lua" func="OnSliderChange" />
					</eventlist>
				</obj>
	
				<obj class="TextObject">
					<attr>
						<left>10</left>
						<top>10+6+8</top>
						<width>48</width>
						<height>14</height>
						<text>进度条1:</text>
					</attr>
					
				</obj>
				
				<obj id="progress1" class="Thunder.Progress">
					<attr>
						<left>10+48</left>
						<top>10+6+8</top>
						
						<height>13</height>
						<Progress>0</Progress>
						<ShowText>true</ShowText>
						<width>300</width>
					</attr>
				</obj>
				
				<obj class="TextObject">
					<attr>
						<left>10</left>
						<top>10+6+8+13+8</top>
						<width>48</width>
						<height>14</height>
						<text>进度条2:</text>
					</attr>	
				</obj>
				
				<obj id="progress2" class="Thunder.Progress">
					<attr>
						<left>10+48</left>
						<top>10+6+8+13+8</top>
						<width>300</width>
						<height>13</height>
						<Progress>0</Progress>
						<ShowText>false</ShowText>
					</attr>
				</obj>
			</children>
		</objtemplate>
	</control>

</xlue>
